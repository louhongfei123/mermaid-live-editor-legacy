{
  "name": "mermaid-live-editor",
  "version": "0.1.0",
  "description": "Edit, preview and share mermaid charts / diagrams.",
  "main": "index.js",
  "scripts": {
    "upgrade": "yarn-upgrade-all",
    "dev": "node -r babel-register node_modules/.bin/webpack-dev-server",
    "test": "standard && jest --coverage",
    "test:watch": "yarn test -- --watch",
    "prepush": "yarn test"
  },
  "repository": "git@github.com:mermaidjs/mermaid-live-editor.git",
  "author": "Tyler Long <tyler4long@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-import": "^1.4.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "nock": "^9.0.14",
    "react-test-renderer": "^15.6.1",
    "redux-logic-test": "^1.0.5",
    "standard": "^10.0.3",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "write-file-webpack-plugin": "^4.1.0",
    "yarn-upgrade-all": "^0.1.6"
  },
  "dependencies": {
    "antd": "^2.12.6",
    "axios": "^0.16.2",
    "mermaid": "^7.0.3",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logic": "^0.12.2",
    "rxjs": "^5.4.3"
  },
  "standard": {
    "ignore": [
      "index.bundle.js"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/identity-obj-proxy/src/index.js"
    },
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  }
}
